{"$schema": "https://json-schema.org/draft/2020-12/schema",
 "$id": "https://github.com/unicode/conformance/likely_subtags_test_schema.json'",
 "title": "ICU Conformance test data description",
 "description": "This documents the format of likely_subtags test data for execution",
 "type": "object",
 "properties": {
   "test_type": {
     "description": "the name likely_subtags",
     "type": "string",
     "enum": ["likely_subtags"]
   },
   "source_file": {
     "description": "origin of conformance data",
     "type": "string"
   },
   "source_version": {
     "description": "version of conformance data",
     "type": "string"
   },
   "tests": {
     "description": "list of N tests for likely subtags",
     "type": "array",
     "items": {
       "type": "object",
       "properties": {
         "label": {
           "description": "A numeric ID, unique for the set of tests",
           "type": "string"
         },
         "locale": {
           "description": "locale tag source ",
           "type": "string"
         },
         "option": {
           "description": "Type of processing requested",
           "type": "string",
           "enum": [
               "maximize",
               "minimize",
               "minimizeFavorScript",
               "minimizeFavorRegion"
             ]
           ]
         }
       }
     },
     "required": [
       "label",
       "locale",
       "option"
     ]
 },
 "required": ["test_type"]
}

